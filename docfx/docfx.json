{
  "metadata": [
    {
      "src": [
        {
          "src": "../.",
          "files": [
            "**/MetasysServices.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Metasys Services",
      "_appTitle": "Metasys Services",
      "_enableSearch": true,
      "_appLogoPath": "images/metasys-logo-small.svg",
      "pdf": false
    }
  }
}
